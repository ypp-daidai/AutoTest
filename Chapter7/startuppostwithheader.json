  {
    "description": "这是一个带有header的post请求",
    "request": {
      "uri": "/post/with/header",
      "method": "post",
      "headers":{
        "content-type": "application/json"
      },
      "json": {
        "name": "daidai",
        "sex": "woman"
      }

